            $(".account").click(function () {
                var X = $(this).attr('id');

                if (X == 1) {
                    $(".submenudrop").hide();
                    $(this).attr('id', '0');
                }
                else {

                    $(".submenudrop").show();
                    $(this).attr('id', '1');
                }

            });

            //Mouseup textarea false
            $(".submenudrop").mouseup(function () {
                return false
            });
            $(".account").mouseup(function () {
                return false
            });


            //Textarea without editing.
            $(document).mouseup(function () {
                $(".submenudrop").hide();
                $(".account").attr('id', '');
            });

        });
        function getautoNumeric(value) {
            $('#txtauto').val('');
            if (value != '') {
                $('#txtauto').autoNumeric("set", value)
                var autone = $('#txtauto').val();
            }
            else {
                return value;
            }
            return autone;
        }

        function getNumeric(value) {
            $('#txtauto').val('');
            if (value != '') {
                $('#txtauto').val(value);                
                var autone = $('#txtauto').autoNumeric("get")
            }
            else {
                return value;
            }
            return autone;
        }
    </script>

