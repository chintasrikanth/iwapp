        function GetPagelevelMenu(FieldName,menuID) {
            try {
                //var FieldName = document.location.href.split('/')[document.location.href.split('/').length - 2] + "/" + document.location.href.split('/')[document.location.href.split('/').length - 1];
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "../CommonWebServices/MasterBindings.aspx/GetPagelevelAccess",
                    data: "{ FieldName: '" + FieldName + "'}",
                    dataType: "json",
                    async: false,
                    success: function (data) {
                        var tdata = jQuery.parseJSON(data.d);
                       
                        if (tdata.length > 0) {

                            if (tdata[0].Authorization == false && tdata[0].Delete == false && tdata[0].Creation == false && tdata[0].Reversal == false) {

                                $("#" + menuID).css('display', 'none')
                            }
                            else {
                                $("#" + menuID).css('display', 'block');


                            }
                            
                        }
                    },
                    error: function (result) {
                        alert('Data not found.');
                    }
                });
            } catch (e) {
                ErrorLog(e);
                alert(ErrorMsg());
            }
        }


        function GetPagelevelAccess() {
            try {
              var FieldName = document.location.href.split('/')[document.location.href.split('/').length - 2] + "/" + document.location.href.split('/')[document.location.href.split('/').length - 1];
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "../CommonWebServices/MasterBindings.aspx/GetPagelevelAccess",
                    data: "{ FieldName: '" + FieldName + "'}",
                    dataType: "json",
                    async: false,
                    success: function (data) {
                        var tdata = jQuery.parseJSON(data.d);
                      
                        if (tdata.length > 0) {
                            tdata[0].Delete == false ? $('.EditClass').css('display', 'none') : $('.EditClass').css('display', 'block');
                            tdata[0].Creation == false ? $('#a_New').css('display', 'none') : '';
                            tdata[0].Creation == false ? $('.ButtonCls').css({ display: "none" }) : '';
                            tdata[0].Creation == false ? $('.submit').css({ display: "none" }) : '';
                            if (tdata[0].Creation == false) {
                                $('#btnFPReview').css('display', 'none');
                                $('#btnAdd').css('display', 'none');
                            }

                            tdata[0].Authorization == false ? $('.AuthorizationClass').css('display', 'none') : $('.AuthorizationClass').css('display', 'block');
                            tdata[0].Delete == false ? $("img[id^='img_Edit_']").css('display', 'none') : $("img[id^='img_Edit_']").css('display', 'block');
                            tdata[0].Reversal == false ? $("img[id^='img_Reverse_']").css('display', 'none') : $("img[id^='img_Reverse_']").css('display', 'block');
                            tdata[0].Reversal == false ? $('.EditClass').css('display', 'none') : $('.EditClass').css('display', 'block');
                            tdata[0].Delete == false ? $('.EditClass').css('display', 'none') : $('.EditClass').css('display', 'block');
                            tdata[0].View == false ? $("img[id^='img_View_']").css('display', 'none') : $("img[id^='img_View_']").css('display', 'block');

                        }
                    },
                    error: function (result) {
                        alert('Data not found.');
                    }
                });
            } catch (e) {
                ErrorLog(e);
                alert(ErrorMsg());
            }
        }

            

